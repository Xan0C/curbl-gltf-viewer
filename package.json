{
  "name": "curbl-gtlf-viewer",
  "version": "0.0.1",
  "private": true,
  "description": "gltf 3d model viewer",
  "main": "index.js",
  "repository": "git@github.com:Xan0C/curbl_gtlf_viewer.git",
  "author": "Soeren <soeren.vu@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-dev": "tsc && webpack --config ./example/webpack.config.js --mode=development",
    "build": "tsc && webpack --config ./example/webpack.config.js --mode=production",
    "start:dev": "webpack-dev-server --config ./example/webpack.config.js",
    "generate-barrels": "barrelsby --delete --location all --directory ./src",
    "eslint": "eslint ./src/**/*.ts ./example/**/*.ts",
    "eslint:fix": "eslint --fix ./src/**/*.ts ./example/**/*.ts",
    "check": "tsc --noEmit && npm run eslint"
  },
  "dependencies": {
    "@curbl/ecs": "^2.2.1",
    "@curbl/gl-util": "^1.0.0",
    "@curbl/loader": "^1.0.0",
    "dat.gui": "^0.7.7",
    "eventemitter3": "^4.0.4",
    "gl-matrix": "3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-classes": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/dat.gui": "^0.7.5",
    "@types/gl-matrix": "^2.4.5",
    "@types/webgl2": "^0.0.5",
    "babel-loader": "^8.1.0",
    "barrelsby": "^2.2.0",
    "compression-webpack-plugin": "^5.0.0",
    "source-map-loader": "^1.0.2",
    "typescript": "^4.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "worker-loader": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5"
  }
}
