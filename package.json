{
  "name": "curbl-gtlf-viewer",
  "version": "0.0.1",
  "private": true,
  "description": "gltf 3d model viewer",
  "main": "index.js",
  "repository": "git@github.com:Xan0C/curbl_gtlf_viewer.git",
  "author": "Soeren <soeren.vu@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-dev": "tsc && webpack --config ./example/webpack.config.js --mode=development",
    "build": "tsc && webpack --config ./example/webpack.config.js --mode=production",
    "start:dev": "webpack-dev-server --config ./example/webpack.config.js",
    "generate-barrels": "barrelsby --delete --location all --directory ./src",
    "eslint": "eslint ./src/**/*.ts ./example/**/*.ts",
    "eslint:fix": "eslint --fix ./src/**/*.ts ./example/**/*.ts",
    "check": "tsc --noEmit && npm run eslint"
  },
  "dependencies": {
    "@curbl/ecs": "^2.1.2",
    "@curbl/gl-util": "^1.0.0",
    "@curbl/loader": "^1.0.0",
    "dat.gui": "^0.7.6",
    "eventemitter3": "^4.0.0",
    "gl-matrix": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/dat.gui": "^0.7.5",
    "@types/gl-matrix": "^2.4.5",
    "@types/webgl2": "^0.0.5",
    "babel-loader": "^8.0.6",
    "barrelsby": "^2.2.0",
    "compression-webpack-plugin": "^3.1.0",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.8.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2",
    "worker-loader": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.18.2"
  }
}
